{
  "name": "redux-react-modals",
  "description": "React lib displaying a layer of modals through redux actions",
  "version": "1.3.0",
  "private": false,
  "main": "lib/index.js",
  "license": "MPL-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/betagouv/redux-react-modals"
  },
  "dependencies": {
    "classnames": "2.2.6",
    "prop-types": "^15.7.2",
    "react": "^16.10.1",
    "react-dom": "^16.10.1",
    "react-redux": "^7.1.0",
    "react-router-dom": "^5.0.0",
    "redux": "^4.0.4"
  },
  "devDependencies": {
    "@babel/core": "^7.0.1",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-cli": "^6.24.1",
    "babel-core": "^7.0.0-0",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^24.9.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.22.0",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "eslint": "^6.2.1",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jest": "^22.7.1",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.13.0",
    "jest": "^24.9.0",
    "jest-environment-jsdom": "^24.5.0",
    "nodemon": "^1.11.0",
    "prettier-eslint-cli": "^5.0.0"
  },
  "keywords": [
    "redux",
    "react",
    "modal",
    "modals"
  ],
  "scripts": {
    "compile": "rm -rf lib && babel --plugins transform-class-properties,transform-object-rest-spread --presets env,react -d lib/ --ignore tests src/",
    "format:js": "prettier-eslint --write './**/*.{js,jsx}'",
    "postinstall": "if [ -d 'src/' ]; then yarn run compile; fi",
    "precommit": "./precommit.sh",
    "prettier": "$(yarn bin)/prettier --write 'src/**/*.js'",
    "prepublish": "yarn run compile",
    "test": "yarn jest --env=jsdom ./src",
    "watch": "nodemon --watch src --exec 'yarn run compile'"
  }
}
