{
  "dependencies": {
    "classnames": "2.2.5",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-redux": "^7.1.0",
    "react-router-dom": "^5.0.0",
    "redux": "^4.0.4"
  },
  "devDependencies": {
    "@babel/core": "^7.0.1",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-cli": "^6.24.1",
    "babel-core": "^7.0.0-0",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^23.4.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.22.0",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "eslint": "^5.16.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jest": "^22.7.1",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.13.0",
    "jest": "^23.4.2",
    "jest-environment-jsdom": "^24.5.0",
    "nodemon": "^1.11.0",
    "prettier": "1.12.1",
    "prettier-eslint-cli": "^5.0.0",
    "prettier-stylelint": "^0.4.2",
    "pretty-quick": "^1.6.0",
    "react-addons-test-utils": "^15.6.2",
    "react-test-renderer": "^16.5.1",
    "stylelint": "^9.4.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-order": "^0.8.1",
    "stylelint-scss": "^3.2.0"
  },
  "keywords": [
    "redux",
    "react",
    "modal",
    "modals"
  ],
  "license": "MPL-2.0",
  "main": "lib/index.js",
  "name": "redux-react-modals",
  "private": false,
  "scripts": {
    "compile": "rm -rf lib && babel --plugins transform-class-properties,transform-object-rest-spread --presets env,react -d lib/ src/",
    "format:js": "prettier-eslint --write './**/*.{js,jsx}'",
    "postinstall": "if [ -d \"src/\" ]; then npm run compile; fi",
    "precommit": "./precommit.sh",
    "prettier": "$(yarn bin)/prettier --write 'src/**/*.js'",
    "prepublish": "npm run compile",
    "test": "yarn jest --env=jsdom ./src",
    "watch": "nodemon --watch src --exec \"npm run compile\""
  },
  "version": "1.0.1"
}
